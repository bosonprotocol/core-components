{
  "type": "object",
  "properties": {
    "schemaUrl": { "type": "string" },
    "type": { "type": "string", "enum": ["BUNDLE"] },
    "name": { "type": "string" },
    "description": { "type": "string" },
    "image": { "type": "string" },
    "externalUrl": { "type": "string" },
    "licenseUrl": { "type": "string" },
    "condition": { "type": "string" },
    "animationUrl": { "type": "string" },
    "attributes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "trait_type": { "type": "string"},
          "value": { "type": "string"},
          "display_type": { "type": "string"}
        },
        "required": ["trait_type", "value"]
      }
    },
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "url": { "type": "string" }
        },
        "required": ["url"]
      },
      "minItems": 1
    }
  },
  "required": ["schemaUrl", "type", "name", "description", "externalUrl", "licenseUrl", "items"]
}
