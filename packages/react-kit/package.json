{
  "name": "@bosonprotocol/react-kit",
  "description": "React toolkit with smart components and hooks for building on top of the Boson Protocol.",
  "version": "0.26.2-alpha.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist/*",
    "src/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@bosonprotocol/chat-sdk": "^1.3.1-alpha.9",
    "@bosonprotocol/core-sdk": "^1.36.2-alpha.0",
    "@bosonprotocol/ethers-sdk": "^1.12.14-alpha.0",
    "@bosonprotocol/ipfs-storage": "^1.11.1-alpha.0",
    "@davatar/react": "1.11.1",
    "@ethersproject/units": "5.6.0",
    "@glidejs/glide": "3.6.0",
    "@rainbow-me/rainbowkit": "^1.3.4",
    "@sentry/browser": "7.43.0",
    "@tippyjs/react": "4.2.6",
    "@uniswap/sdk-core": "^4.0.7",
    "chart.js": "^4.4.1",
    "dayjs": "1.11.7",
    "eth-revert-reason": "^1.0.3",
    "formik": "2.2.9",
    "graphql-request": "5.2.0",
    "lodash.merge": "4.6.2",
    "lodash.uniqby": "4.7.0",
    "magic-sdk": "^20.1.1",
    "phosphor-react": "1.4.1",
    "polished": "^4.2.2",
    "react-chartjs-2": "^5.2.0",
    "react-hot-toast": "2.4.0",
    "react-markdown": "8.0.6",
    "react-phone-number-input": "3.2.19",
    "react-query": "3.39.3",
    "react-responsive": "9.0.2",
    "react-router-dom": "6.9.0",
    "react-select": "5.7.0",
    "react-slick": "^0.29.0",
    "react-table": "7.8.0",
    "rehype-raw": "6.1.1",
    "remark-gfm": "3.0.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^6.1.1",
    "stylis": "^4.3.0",
    "tippy.js": "6.3.7",
    "use-async-effect": "2.2.7",
    "utility-types": "3.10.0",
    "viem": "^1.21.4",
    "wagmi": "^1.4.13",
    "yup": "0.32.11"
  },
  "peerDependencies": {
    "ethers": "^5.7.2",
    "react": "17 - 18",
    "react-dom": "17 - 18"
  },
  "scripts": {
    "lint": "eslint --ignore-path ../../.gitignore --ext .jsx,.tsx .",
    "eject": "react-scripts eject",
    "clean": "rimraf build coverage .turbo node_modules",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build": "rimraf dist && tsc && tsc --build tsconfig.cjs.json",
    "copy-assets": "copyfiles -f src/assets/** dist/esm/assets && copyfiles -f src/assets/** dist/cjs/assets",
    "postbuild": "npm run copy-assets"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc",
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^@bosonprotocol/(.*)$": "<rootDir>/../$1/"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/glidejs__glide": "^3.6.1",
    "@types/lodash.merge": "^4.6.7",
    "@types/lodash.uniqby": "^4.7.7",
    "@types/node": "^16.11.25",
    "@types/react": "^18.0.15",
    "@types/react-slick": "^0.23.13",
    "@types/react-table": "^7.7.14",
    "@types/storybook__react": "^5.2.1",
    "@web3-react/core": "8.2.3",
    "@web3-react/metamask": "8.2.4",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babel-plugin-styled-components": "^2.0.7",
    "buffer": "6.0.3",
    "copyfiles": "^2.4.1",
    "customize-cra": "^1.0.0",
    "eslint": "^8.10.0",
    "file-loader": "^6.2.0",
    "jest": "^29.6.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-docgen-typescript-plugin": "^1.0.5",
    "react-scripts": "5.0.0",
    "rimraf": "^3.0.2",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "overrides": {
    "typescript": "^5.1.6"
  }
}
