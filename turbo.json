{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "test": {},
    "lint": {},
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^dev"]
    }
  }
}
